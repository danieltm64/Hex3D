                <property name="translation" value="0 0 0" />
                <property name="scale" value="1 1 1" />
            </component>
            <children>
                <entity name="camera1">
                    <component type="camera">
                        <property name="eye_position" value="0 10 -10" />
                        <property name="target_position" value="0 0 0" />
                        <property name="up_direction" value="0 1 0" />
                        <property name="vertical_FOV" value="1.256" />
                        <property name="aspect_ratio" value="1.333" />
                        <property name="near_z" value="1" />
                        <property name="far_z" value="1000" />
                    </component>
                </entity>
                <entity name="cube1">
                    <component type="transform">
                        <property name="translation" value="0 0 0" />
                        <property name="scale" value="1 1 1" />
                    </component>
                    <component type="cube" />
                    <children>
                        <entity name="subcube11">
                            <component type="transform">
                                <property name="translation" value="10 0 0" />
                                <property name="scale" value="1 1 1" />
                            </component>
                            <component type="cube" />
                        </entity>
                    </children>
                </entity>
            </children>
        </entity>
    </scene_graph>
</level>
